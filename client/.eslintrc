{
  "parser": "@babel/eslint-parser",
  "extends": [
    "react-app",
    "react-app/jest",
  ],
  "plugins": [
    "react",
    "prettier"
  ],
  "parserOptions": {
    "requireConfigFile": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
