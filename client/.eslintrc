{
    "parser": "@babel/eslint-parser",
    "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended"],
    "plugins": ["react", "prettier"],
    "parserOptions": {
        "requireConfigFile": true
    },
    "rules": {
      "linebreak-style": ["error", "unix"],
      "indent": ["error", 4]
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    }
}
